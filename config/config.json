{
  "datasets": {
    "DataSetNClassRandom": {
      "type": "NClassRandomClassificationWithNoise",
      "parameters": {
        "n_samples": 100,
        "shuffle": true,
        "noise": 0,
        "random_state": 0
      }
    },
    "DataSetCircle": {
      "type": "make_circles",
      "parameters": {
        "n_samples": 100,
        "shuffle": true,
        "noise": 0,
        "random_state": 0,
        "factor": 0.3
      }
    },
    "DataSetIris": {
      "type": "Iris",
      "parameters": {}
    }
  },
  "algorithms": {
    "knn-base": {
      "algorithm-family": "sklearn.neighbors.KNeighborsClassifier",
      "hyperparameters": {
        "n_neighbors": 2,
        "algorithm": "auto",
        "weights": "uniform"
      }
    },
    "smv-base": {
      "algorithm-family": "sklearn.svm.SVC",
      "hyperparameters": {
        "C": 0.9,
        "kernel": "rbf"
      }
    }
  },
  "splits": {
    "traintest20": {
      "type": "traintest",
      "parameters": {
        "test_size": 20,
        "random_state": 0,
        "shuffle": true
      }
    }
  },
  "learning_process": {
    "input": [
      "DataSetCircle",
      "DataSetNClassRandom"
    ],
    "split": "traintest20",
    "algorithm": [
      "knn-base",
      "smv-base"
    ]
  }
}