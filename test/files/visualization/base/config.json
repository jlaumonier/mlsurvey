{
  "datasets": {
    "DataSetNClassRandom": {
      "type": "NClassRandomClassificationWithNoise",
      "parameters": {
        "n_samples": 100,
        "shuffle": true,
        "noise": 0,
        "random_state": 0
      }
    }
  },
  "algorithms": {
    "knn-base": {
      "algorithm-family": "sklearn.neighbors.KNeighborsClassifier",
      "hyperparameters": {
        "n_neighbors": 2,
        "algorithm": "auto",
        "weights": "uniform"
      }
    }
  },
  "splits": {
    "traintest20": {
      "type": "traintest",
      "parameters": {
        "test_size": 20,
        "random_state": 0,
        "shuffle": true
      }
    }
  },
  "learning_process": {
    "input": "DataSetNClassRandom",
    "split": "traintest20",
    "algorithm": "knn-base"
  }
}