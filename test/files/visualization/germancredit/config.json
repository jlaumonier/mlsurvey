{
  "datasets": {
    "DataSetGermanCredit": {
      "type": "FileDataSet",
      "parameters": {
        "directory": "files/dataset",
        "filename": "credit-g.arff"
      },
      "fairness": {
        "protected_attribute": 12,
        "privileged_classes": "x >= 25"
      }
    }
  },
  "algorithms": {
    "smv-base": {
      "algorithm-family": "sklearn.svm.SVC",
      "hyperparameters": {
        "C": 0.9,
        "kernel": "rbf",
        "gamma": "auto"
      }
    }
  },
  "splits": {
    "traintest20": {
      "type": "traintest",
      "parameters": {
        "test_size": 20,
        "random_state": 0,
        "shuffle": true
      }
    }
  },
  "learning_process": {
    "input": "DataSetGermanCredit",
    "split": "traintest20",
    "algorithm": "smv-base"
  }
}