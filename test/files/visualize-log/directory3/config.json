{
  "datasets": {
    "DataSetNClassRandom": {
      "type": "NClassRandomClassificationWithNoise",
      "parameters": {
        "n_samples": 10000,
        "shuffle": true,
        "noise": 0,
        "random_state": 0
      }
    }
  },
  "algorithms": {
    "nn-base": {
      "algorithm-family": "sklearn.neural_network.MLPClassifier",
      "hyperparameters": {
        "activation": "tanh",
        "max_iter": 2000
      }
    }
  },
  "splits": {
    "traintest20": {
      "type": "traintest",
      "parameters": {
        "test_size": 20,
        "random_state": 0,
        "shuffle": true
      }
    }
  },
  "learning_process": {
    "input": "DataSetNClassRandom",
    "split": "traintest20",
    "algorithm": "nn-base"
  }
}