{
  "datasets": {
    "DataSetGermanCredit": {
      "type": "FileDataSet",
      "parameters": {
        "directory": "files/dataset",
        "filename": "test-fairness.arff"
      },
      "fairness": {
        "protected_attribute": 1,
        "privileged_classes": [
          "x >= 25",
          "x >= 35"
        ]
      }
    }
  },
  "algorithms": {
    "knn-base": {
      "algorithm-family": "sklearn.neighbors.KNeighborsClassifier",
      "hyperparameters": {
        "n_neighbors": 2,
        "algorithm": "auto",
        "weights": "uniform"
      }
    }
  },
  "splits": {
    "traintest20": {
      "type": "traintest",
      "parameters": {
        "test_size": 5,
        "random_state": 0,
        "shuffle": true
      }
    }
  },
  "learning_process": {
    "parameters": {
      "input": "DataSetGermanCredit",
      "split": "traintest20",
      "algorithm": "knn-base"
    }
  }
}