{
  "datasets": {
    "DataSetDE": {
      "type": "FileDataSet",
      "storage": "Dask",
      "parameters": {
        "directory": "files/dataset",
        "filename": "unittest_de.csv"
      },
      "metadata": {
        "y_col_name": "rating"
      }
    }
  },
  "algorithms": {
    "svm-base": {
      "algorithm-family": "sklearn.svm.SVC",
      "hyperparameters": {
        "C": 0.9,
        "kernel": "rbf",
        "gamma": "auto"
      }
    }
  },
  "splits": {
    "traintest20": {
      "type": "traintest",
      "parameters": {
        "test_size": 5,
        "random_state": 0,
        "shuffle": true
      }
    }
  },
  "learning_process": {
    "input": "DataSetDE",
    "split": "traintest20",
    "algorithm": "svm-base"
  }
}