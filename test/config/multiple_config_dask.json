{
  "datasets": {
    "DataSet1": {
      "type": "NClassRandomClassificationWithNoise",
      "storage": "Dask",
      "parameters": {
        "n_samples": 100,
        "shuffle": true,
        "random_state": 0,
        "noise": 0
      }
    },
    "DataSet2": {
      "type": "make_circles",
      "storage": "Dask",
      "parameters": {
        "n_samples": 100,
        "shuffle": true,
        "noise": 0,
        "random_state": 0,
        "factor": 0.3
      }
    },
    "DataSetDE": {
      "type": "FileDataSet",
      "storage": "Dask",
      "parameters": {
        "directory": "files/dataset",
        "filename": "unittest_de.csv"
      },
      "metadata": {
        "y_col_name": "rating"
      }
    }
  },
  "algorithms": {
    "knn-base": {
      "algorithm-family": "sklearn.neighbors.KNeighborsClassifier",
      "hyperparameters": {
        "n_neighbors": 15,
        "algorithm": "auto",
        "weights": "uniform"
      }
    }
  },
  "splits": {
    "traintest20": {
      "type": "traintest",
      "parameters": {
        "test_size": 20,
        "random_state": 0,
        "shuffle": true
      }
    }
  },
  "learning_process": {
    "input": [
      "DataSet1",
      "DataSet2",
      "DataSetDE"
    ],
    "split": [
      "traintest20"
    ],
    "algorithm": [
      "knn-base"
    ]
  }
}