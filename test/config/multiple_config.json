{
  "#refs": {
    "datasets": {
      "DataSet1": {
        "type": "NClassRandomClassificationWithNoise",
        "parameters": {
          "n_samples": 100,
          "shuffle": true,
          "random_state": 0,
          "noise": 0
        }
      },
      "DataSet2": {
        "type": "make_circles",
        "parameters": {
          "n_samples": 100,
          "shuffle": true,
          "noise": 0,
          "random_state": 0,
          "factor": 0.3
        }
      },
      "DataSet3": {
        "type": "load_iris",
        "parameters": {
        }
      },
      "DataSet4": {
        "type": "FileDataSet",
        "parameters": {
          "directory": "files/dataset",
          "filename": "test-fairness.arff"
        },
        "metadata": {
          "y_col_name": "class"
        }
      }
    },
    "algorithms": {
      "knn-base": {
        "type": "sklearn.neighbors.KNeighborsClassifier",
        "hyperparameters": {
          "n_neighbors": 2,
          "algorithm": "auto",
          "weights": "uniform"
        }
      }
    },
    "splits": {
      "traintest20": {
        "type": "traintest",
        "parameters": {
          "test_size": 5,
          "random_state": 0,
          "shuffle": true
        }
      }
    }
  },
  "learning_process": {
    "type": "mlsurvey.sl.workflows.SupervisedLearningWorkflow",
    "parameters": {
      "input": [
        "@datasets.DataSet1",
        "@datasets.DataSet2",
        "@datasets.DataSet3",
        "@datasets.DataSet4"
      ],
      "split": [
        "@splits.traintest20"
      ],
      "algorithm": [
        "@algorithms.knn-base"
      ]
    }
  }
}